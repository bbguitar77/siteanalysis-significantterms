  <!-- Start 'Built for developers' -->
  <article id="built-for-developers">
    <header>
      <h2>Built for developers</h2>
      <p>Robust, clean APIs that let you focus on building great products.</p>
    </header>

    <div class="examples inner-col smaller">

<script type='text/template' class='curl'><span class="keyword">curl</span> <span style="color:rgba(34, 135, 34, 1)">https://api.stripe.com/v1/charges</span> \
  -u <span class="string">sk_test_BQokikJOvBiI2HlWgH4olfQ2</span>: \
  -d <span class="symbol">amount</span>=<span class="string">400</span> \
  -d <span class="symbol">currency</span>=<span class="string">usd</span> \
  -d "<span class="symbol">description</span>=<span class="string">Charge for test@example.com</span>" \
  -d "<span class="symbol">card[number]</span>=<span class="string">4242424242424242</span>" \
  -d "<span class="symbol">card[exp_month]</span>=<span class="string">12</span>" \
  -d "<span class="symbol">card[exp_year]</span>=<span class="string">2014</span>" \
  -d "<span class="symbol">card[cvc]</span>=<span class="string">123</span>"</script>

      <script type="text/javascript">
        window.APIValues = {};

          APIValues["ruby"] = "\u003Cspan class=\"highlight_js ruby\"\u003Erequire \u0026quot;stripe\u0026quot;\nStripe.api_key = \u0026quot;sk_test_BQokikJOvBiI2HlWgH4olfQ2\u0026quot;\n\nStripe::Charge.create(\n  :amount =\u0026gt; 400,\n  :currency =\u0026gt; \u0026quot;usd\u0026quot;,\n  :card =\u0026gt; {\n    :number =\u0026gt; \u0026quot;4242424242424242\u0026quot;,\n    :exp_month =\u0026gt; 8,\n    :exp_year =\u0026gt; 2015,\n    :cvc =\u0026gt; \u0026quot;314\u0026quot;\n  },\n  :description =\u0026gt; \u0026quot;Charge for test@example.com\u0026quot;\n)\u003C/span\u003E";
          APIValues["python"] = "\u003Cspan class=\"highlight_js python\"\u003Eimport stripe\nstripe.api_key = \u0026quot;sk_test_BQokikJOvBiI2HlWgH4olfQ2\u0026quot;\n\nstripe.Charge.create(\n  amount=400,\n  currency=\u0026quot;usd\u0026quot;,\n  card={\n    \u0026quot;number\u0026quot;: \u0026#39;4242424242424242\u0026#39;,\n    \u0026quot;exp_month\u0026quot;: 12,\n    \u0026quot;exp_year\u0026quot;: 2015,\n    \u0026quot;cvc\u0026quot;: \u0026#39;123\u0026#39;\n  },\n  description=\u0026quot;Charge for test@example.com\u0026quot;\n)\u003C/span\u003E";
          APIValues["php"] = "\u003Cspan class=\"highlight_js php\"\u003Erequire_once(\u0026#39;./lib/Stripe.php\u0026#39;);\nStripe::setApiKey(\u0026quot;sk_test_BQokikJOvBiI2HlWgH4olfQ2\u0026quot;);\n\nStripe_Charge::create(array(\n  \u0026quot;amount\u0026quot; =\u0026gt; 400,\n  \u0026quot;currency\u0026quot; =\u0026gt; \u0026quot;usd\u0026quot;,\n  \u0026quot;card\u0026quot; =\u0026gt; array(\n    \u0026quot;number\u0026quot; =\u0026gt; \u0026quot;4242424242424242\u0026quot;,\n    \u0026quot;exp_month\u0026quot; =\u0026gt; 8,\n    \u0026quot;exp_year\u0026quot; =\u0026gt; 2015,\n    \u0026quot;cvc\u0026quot; =\u0026gt; \u0026quot;314\u0026quot;\n  ),\n  \u0026quot;description\u0026quot; =\u0026gt; \u0026quot;Charge for test@example.com\u0026quot;\n));\u003C/span\u003E";
          APIValues["java"] = "\u003Cspan class=\"highlight_js java\"\u003EStripe.apiKey = \u0026quot;sk_test_BQokikJOvBiI2HlWgH4olfQ2\u0026quot;;\n\nMap\u0026lt;String, Object\u0026gt; chargeParams = new HashMap\u0026lt;String, Object\u0026gt;();\nchargeParams.put(\u0026quot;amount\u0026quot;, 400);\nchargeParams.put(\u0026quot;currency\u0026quot;, \u0026quot;usd\u0026quot;);\nMap\u0026lt;String, Object\u0026gt; cardParams = new HashMap\u0026lt;String, Object\u0026gt;();\ncardParams.put(\u0026quot;number\u0026quot;, \u0026quot;4242424242424242\u0026quot;);\ncardParams.put(\u0026quot;exp_month\u0026quot;, 8);\ncardParams.put(\u0026quot;exp_year\u0026quot;, 2015);\ncardParams.put(\u0026quot;cvc\u0026quot;, \u0026quot;314\u0026quot;);\nchargeParams.put(\u0026quot;card\u0026quot;, cardParams);\nchargeParams.put(\u0026quot;description\u0026quot;, \u0026quot;Charge for test@example.com\u0026quot;);\n\nCharge.create(chargeParams);\u003C/span\u003E";
          APIValues["node"] = "\u003Cspan class=\"highlight_js javascript\"\u003Evar stripe = require(\u0026quot;stripe\u0026quot;)(\n  \u0026quot;sk_test_BQokikJOvBiI2HlWgH4olfQ2\u0026quot;\n);\n\nstripe.charges.create({\n  amount: 400,\n  currency: \u0026quot;usd\u0026quot;,\n  card: {\n    number: \u0026#39;4242424242424242\u0026#39;,\n    exp_month: 12,\n    exp_year: 2015,\n    cvc: \u0026#39;123\u0026#39;\n  },\n  description: \u0026quot;Charge for test@example.com\u0026quot;\n}, function(err, charge) {\n  // asynchronously called\n});\u003C/span\u003E";
      </script>

      <ul>
        <li>curl</li>
          <li>Ruby</li>
          <li>Python</li>
          <li>PHP</li>
          <li>Java</li>
          <li>Node</li>
      </ul>

